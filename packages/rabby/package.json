{
  "name": "@windoge98/pnp-rabby",
  "version": "0.1.0-beta.17",
  "description": "Rabby wallet adapter for PNP using SIWE",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
    }
  },
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "cd ../.. && BUILD_PACKAGE=rabby vite build",
    "dev": "vite",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "ic-siwe-js": "^0.2.5",
    "viem": "^2.21.62"
  },
  "peerDependencies": {
    "@windoge98/plug-n-play": ">=0.1.0-beta",
    "@dfinity/agent": "^2.4.1",
    "@dfinity/identity": "^2.4.1",
    "@dfinity/principal": "^2.4.1"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  },
  "publishConfig": {
    "access": "public"
  }
}