    "dev": "cd ../.. && BUILD_PACKAGE=ethereum vite"
  },
  "dependencies": {
    "@dfinity/agent": "^2.4.1",
    "@dfinity/identity": "^2.4.1",
    "@dfinity/principal": "^2.4.1",
    "@walletconnect/ethereum-provider": "^2.21.8",
    "ethers": "^6.15.0",
    "ic-siwe-js": "^0.2.5",
    "viem": "^2.33.3"
  },
  "devDependencies": {
    "vite": "^7.1.2",
    "vite-plugin-dts": "^4.5.4"
  },
  "license": "MIT"
}
