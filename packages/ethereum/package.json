{
  "name": "@windoge98/pnp-ethereum",
  "version": "0.0.1-beta.4",
  "description": "Ethereum wallet adapter for PNP (Sign-In with Ethereum)",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.es.js",
      "require": "./dist/index.js"
