{
  "name": "@windoge98/pnp-metamask",
  "version": "0.1.0-beta.7",
  "description": "MetaMask wallet adapter for PNP using SIWE",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "types": "dist/index.d.ts",
