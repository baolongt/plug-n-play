  },
  "dependencies": {
    "@windoge98/plug-n-play": "file:../..",
    "@dfinity/agent": "^2.4.1",
    "@dfinity/identity": "^2.4.1",
    "@dfinity/principal": "^2.4.1",
    "ic-siwe-js": "^0.2.5",
    "viem": "^2.21.62"
  },
  "devDependencies": {
    "typescript": "^5.9.2",
    "vite": "^7.1.2"
  }
}